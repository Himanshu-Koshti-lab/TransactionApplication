<configuration debug ="true">
    <appender name="CONSOLE-LOG" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
        <pattern>%d{HH:mm:ss}</pattern>
        </encoder>

    </appender>
    <root level="INFO">
        <appender-ref ref="CONSOLE-LOG"/>
    </root>
</configuration>